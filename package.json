{
  "private": true,
  "scripts": {
    "ci": "turbo run build e2e:test lint",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "dev:test": "turbo run dev:test",
    "db:seed": "pnpm --filter be db:seed",
    "e2e:test": "turbo run e2e:test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "find . -name node_modules -o -name .next -o -name .expo -o -name .turbo -o -name dist -type d -prune | xargs rm -rf && find . -name tsconfig.tsbuildinfo | xargs rm -f"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "turbo-playwright-e2e-tests"
}
